{
	"license": "MIT",
	"name": "wonky-line",
	"version": "0.0.3",
	"description": "For when you want a line, but you know, a bit wonky",
	"keywords": [
		"wonky",
		"line",
		"svg",
		"react"
	],
	"homepage": "https://github.com/deansimcox/wonky-line",
	"bugs": {
		"url": "https://github.com/deansimcox/wonky-line/issues"
	},
	"author": "Dean Simcox",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/deansimcox/wonky-line.git"
	},
	"files": [
		"dist"
	],
	"type": "module",
	"main": "./dist/index.js",
	"scripts": {
		"build": "tsc",
		"ci": "npm run build && npm run format-check && npm run check-exports && npm run test",
		"format": "biome check --write",
		"format-check": "biome check --write",
		"check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
		"test": "vitest run",
		"test:watch": "vitest watch",
		"local-release": "changeset version && changeset publish",
		"prepublishOnly": "npm run ci"
	},
	"devDependencies": {
		"@arethetypeswrong/cli": "^0.17.4",
		"@biomejs/biome": "1.9.4",
		"@changesets/cli": "^2.28.1",
		"@types/react": "^19.1.0",
		"typescript": "^5.8.3",
		"vitest": "^3.1.1"
	},
	"dependencies": {
		"@emotion/css": "^11.13.5",
		"@emotion/styled": "^11.14.0",
		"react": "^19.1.0",
		"react-dom": "^19.1.0"
	}
}